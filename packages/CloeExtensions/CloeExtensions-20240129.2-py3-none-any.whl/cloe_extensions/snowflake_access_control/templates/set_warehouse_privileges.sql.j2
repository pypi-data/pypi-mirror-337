{% if usage is not none -%}
{{ 'GRANT' if usage else "REVOKE" }} USAGE ON WAREHOUSE {{ warehouse_name }} {{ 'TO' if usage else 'FROM' }} ROLE {{ '"' ~ role_name ~ '"' if use_doublequotes_for_name else  role_name }};
