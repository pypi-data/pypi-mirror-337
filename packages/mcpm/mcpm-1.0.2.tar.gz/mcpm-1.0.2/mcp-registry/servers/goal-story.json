  "installations": {
    "npm": {
      "type": "npm",
      "command": "npx",
      "args": [
        "-y",
        "goalstory-mcp",
        "https://prod-goalstory-rqc2.encr.app",
        "${YOUR_API_KEY}"
      ]
    }
  },
  "arguments": {
    "YOUR_API_KEY": {
      "description": "The API key required to authenticate your requests to the Goal Story service.",
      "required": true,
      "example": "abcdefgh12345678"
    }
  }
}