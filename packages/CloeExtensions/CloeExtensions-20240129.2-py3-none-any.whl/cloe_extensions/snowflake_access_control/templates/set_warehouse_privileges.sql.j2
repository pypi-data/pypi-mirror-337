{{ 'GRANT' if operate else "REVOKE" }} OPERATE ON WAREHOUSE {{ warehouse_name }} {{ 'TO' if operate else 'FROM' }} ROLE {{ '"' ~ role_name ~ '"' if use_doublequotes_for_name else  role_name }};
{% endif -%}
