<odoo>
    <record
        id="account_invoice_report_commercial_address_view_move_form"
        model="ir.ui.view"
    >
        <field
            name="name"
        >account.invoice.report.commercial.address.view.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='to_check']" position="after">
                <field name="show_commercial_entity" />
            </xpath>
        </field>
    </record>
</odoo>
